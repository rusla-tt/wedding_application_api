<h1>Listing person_scores</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Score</th>
      <th>Hi score</th>
      <th>Create</th>
      <th>Update</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person_scores.each do |person_score| %>
      <tr>
        <td><%= person_score.name %></td>
        <td><%= person_score.score %></td>
        <td><%= person_score.hi_score %></td>
        <td><%= person_score.create %></td>
        <td><%= person_score.update %></td>
        <td><%= link_to 'Show', person_score %></td>
        <td><%= link_to 'Edit', edit_person_score_path(person_score) %></td>
        <td><%= link_to 'Destroy', person_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person score', new_person_score_path %>
